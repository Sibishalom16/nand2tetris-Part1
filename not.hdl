// File name: tools/builtInChips/Not.hdl
 /**
 * Not gate:
 * out = ((in == 0), 1, 0)
 */
 CHIP Not {

    IN  in;
    OUT out;

     PARTS:
     Not(in=a, out=nota);
     Not(in=b, out=notb);
     And(a=nota, b=notb, out=c);
 }
